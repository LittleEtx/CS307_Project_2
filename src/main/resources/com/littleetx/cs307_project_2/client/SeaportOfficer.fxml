<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<VBox fx:id="rootVBox" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="600.0" prefWidth="800.0" spacing="20.0" xmlns="http://javafx.com/javafx/19"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.littleetx.cs307_project_2.client.controllers.SeaportOfficerController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <children>
        <TabPane fx:id="tabPane" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
            <tabs>
                <Tab fx:id="importTab" onSelectionChanged="#onRefreshClick" text="Import">
                    <content>
                        <HBox fx:id="importHBox">
                            <children>
                                <VBox alignment="TOP_CENTER" minWidth="100.0" prefHeight="200.0" prefWidth="100.0"
                                      spacing="10.0">
                                    <children>
                                        <Button mnemonicParsing="false" onAction="#onRefreshClick" prefWidth="80.0"
                                                textAlignment="CENTER">
                                            <graphic>
                                                <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true"
                                                           preserveRatio="true">
                                                    <image>
                                                        <Image url="@../assets/icons/refresh.png"/>
                                                    </image>
                                                </ImageView>
                                            </graphic>
                                        </Button>
                                        <TextField fx:id="searchImport" promptText="search"/>
                                        <Button fx:id="importPassBtn" mnemonicParsing="false"
                                                onAction="#onPassImportItemCheck" prefWidth="80.0"
                                                styleClass="ok-button" stylesheets="@../assets/css/ButtonCSS.css"
                                                text="Pass" textAlignment="CENTER"/>
                                        <Button fx:id="importRejectBtn" mnemonicParsing="false"
                                                onAction="#onRejectImportItemCheck" prefWidth="80.0"
                                                styleClass="cancel-button" stylesheets="@../assets/css/ButtonCSS.css"
                                                text="Reject" textAlignment="CENTER"/>
                                    </children>
                                    <padding>
                                        <Insets left="10.0" right="10.0" top="20.0"/>
                                    </padding>
                                </VBox>
                            </children>
                        </HBox>
                    </content>
                </Tab>
                <Tab fx:id="exportTab" onSelectionChanged="#onRefreshClick" text="Export">
                    <content>
                        <HBox fx:id="exportHBox">
                            <children>
                                <VBox alignment="TOP_CENTER" minWidth="100.0" prefHeight="200.0" prefWidth="100.0"
                                      spacing="10.0">
                                    <children>
                                        <Button mnemonicParsing="false" onAction="#onRefreshClick" prefWidth="80.0"
                                                textAlignment="CENTER">
                                            <graphic>
                                                <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true"
                                                           preserveRatio="true">
                                                    <image>
                                                        <Image url="@../assets/icons/refresh.png"/>
                                                    </image>
                                                </ImageView>
                                            </graphic>
                                        </Button>
                                        <TextField fx:id="searchExport" promptText="search"/>
                                        <Button fx:id="exportPassBtn" mnemonicParsing="false"
                                                onAction="#onPassExportItemCheck" prefWidth="80.0"
                                                styleClass="ok-button" stylesheets="@../assets/css/ButtonCSS.css"
                                                text="Pass" textAlignment="CENTER"/>
                                        <Button fx:id="exportRejectBtn" mnemonicParsing="false"
                                                onAction="#onRejectExportItemCheck" prefWidth="80.0"
                                                styleClass="cancel-button" stylesheets="@../assets/css/ButtonCSS.css"
                                                text="Reject" textAlignment="CENTER"/>
                                    </children>
                                    <padding>
                                        <Insets left="10.0" right="10.0" top="20.0"/>
                                    </padding>
                                </VBox>
                            </children>
                        </HBox>
                    </content>
                </Tab>
                <Tab fx:id="finishedTab" onSelectionChanged="#onRefreshClick" text="Finished">
                    <content>
                        <HBox fx:id="finishedHBox" prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
                                    <children>
                                        <Button mnemonicParsing="false" onAction="#onRefreshClick" prefWidth="80.0"
                                                textAlignment="CENTER">
                                            <graphic>
                                                <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true"
                                                           preserveRatio="true">
                                                    <image>
                                                        <Image url="@../assets/icons/refresh.png"/>
                                                    </image>
                                                </ImageView>
                                            </graphic>
                                        </Button>
                                        <TextField fx:id="searchFinished" promptText="search"/>
                                    </children>
                                    <padding>
                                        <Insets left="10.0" right="10.0" top="20.0"/>
                                    </padding>
                                </VBox>
                            </children>
                        </HBox>
                    </content>
                </Tab>
                <Tab fx:id="taxTab" onSelectionChanged="#onRefreshClick" text="Tax Rate">
                    <content>
                        <HBox fx:id="taxHBox" prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
                                    <children>
                                        <Button mnemonicParsing="false" onAction="#onRefreshClick" prefWidth="80.0"
                                                textAlignment="CENTER">
                                            <graphic>
                                                <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true"
                                                           preserveRatio="true">
                                                    <image>
                                                        <Image url="@../assets/icons/refresh.png"/>
                                                    </image>
                                                </ImageView>
                                            </graphic>
                                        </Button>
                                    </children>
                                    <padding>
                                        <Insets top="20.0"/>
                                    </padding>
                                </VBox>
                            </children>
                        </HBox>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</VBox>
